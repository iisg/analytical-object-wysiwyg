{
  "name": "analytical-object-wysiwyg",
  "version": "0.1.0",
  "authors": [
    "Amadeusz Starzykiewicz <megawebmaster@gmail.com>"
  ],
  "description": "Preconfigured WYSIWYG editor using Quill for using analytical objects.",
  "main": [
    "dist/module.js",
    "dist/templates.min.js"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/iisg/analytical-object-wysiwyg"
  },
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "quill",
    "format",
    "agh",
    "objects",
    "configurable"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "coffeelint.json",
    "Gulpfile.coffee",
    "Gulpfile.js",
    "package.json"
  ],
  "dependencies": {
    "quill": "megawebmaster/quill#dist",
    "jquery": "2.1.4"
  },
  "override": {
    "quill": {
      "main": ["dist/quill.js", "dist/quill.base.css"]
    }
  },
  "resolve": {
    "quill": "dist"
  }
}
